<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/niklr/XDASv2Net</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/niklr/XDASv2Net/blob/master/LICENSE.txt</PackageLicenseUrl>
    <Description>XDASv2Net is a .NET library containing the model of the XDASv2 specification.</Description>
    <PackageTags>XDASv2</PackageTags>
    <Authors>niklr</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
  </ItemGroup>

</Project>
